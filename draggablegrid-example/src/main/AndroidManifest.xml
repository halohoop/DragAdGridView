            </intent-filter>-->
        </activity>
        <activity android:name="net.halohoop.dragadgridview.activities.MainAdActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

